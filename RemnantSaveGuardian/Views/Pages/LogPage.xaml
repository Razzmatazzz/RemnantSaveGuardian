<ui:UiPage
    x:Class="RemnantSaveGuardian.Views.Pages.LogPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:RemnantSaveGuardian.Views.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:models="clr-namespace:RemnantSaveGuardian.Models"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    Title="LogPage"
    d:DataContext="{d:DesignInstance local:DataPage,
                                     IsDesignTimeCreatable=False}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    d:Background="{DynamicResource ApplicationBackgroundBrush}"
    Foreground="{DynamicResource TextFillColorPrimaryBrush}"
    mc:Ignorable="d"
    Scrollable="True">

    <StackPanel Name="stackLogs" Margin="23,0" Height="Auto">
    </StackPanel>
</ui:UiPage>
